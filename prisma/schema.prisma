model ListingAdStat {
  id                  BigInt     @id @default(autoincrement())
  listingId           BigInt     @map("listing_id")
  periodStart         DateTime   @map("period_start")
  periodEnd           DateTime   @map("period_end")
  impressions         BigInt?
  clicks              BigInt?
  orders              BigInt?
  revenue             Float?
  revenueDivisor      Int?       @map("revenue_divisor")
  revenueCurrencyCode String?    @map("revenue_currency_code")
  spend               Float?
  spendDivisor        Int?       @map("spend_divisor")
  spendCurrencyCode   String?    @map("spend_currency_code")
  rawStats            String?    @map("raw_stats")
  createdAt           DateTime   @default(now()) @map("created_at")
  updatedAt           DateTime   @updatedAt @map("updated_at")
  periodType          PeriodType @default(MONTHLY)
  listing             Listing    @relation(fields: [listingId], references: [listingId], onDelete: Cascade)

  @@unique([listingId, periodType, periodStart, periodEnd])
  @@index([listingId], map: "idx_listing_ad_stats_listing_id")
  @@index([listingId, periodStart, periodEnd], map: "idx_listing_ad_stats_listing_period")
  @@index([periodEnd], map: "idx_listing_ad_stats_period_end")
  @@index([periodStart], map: "idx_listing_ad_stats_period_start")
  @@map("listing_ad_stats")
}

model ListingVisitStat {
  id          BigInt     @id @default(autoincrement())
  listingId   BigInt     @map("listing_id")
  periodType  PeriodType
  periodStart DateTime   @map("period_start")
  periodEnd   DateTime   @map("period_end")
  visits      BigInt?
  views       BigInt?
  orders      BigInt?
  revenue     Float?
  rawResponse String?    @map("raw_response")
  createdAt   DateTime   @default(now()) @map("created_at")
  updatedAt   DateTime   @updatedAt @map("updated_at")
  listing     Listing    @relation(fields: [listingId], references: [listingId], onDelete: Cascade)

  @@unique([listingId, periodType, periodStart, periodEnd])
  @@index([listingId], map: "idx_listing_visit_stats_listing_id")
  @@index([listingId, periodStart, periodEnd], map: "idx_listing_visit_stats_listing_period")
  @@index([periodEnd], map: "idx_listing_visit_stats_period_end")
  @@index([periodStart], map: "idx_listing_visit_stats_period_start")
  @@map("listing_visit_stats")
}

enum PeriodType {
  YEARLY
  MONTHLY
  WEEKLY
}
